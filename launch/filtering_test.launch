<launch>
    <param name="/use_sim_time" value="true" />

    <arg name="map_frame"   default="map"/>
    <arg name="odom_frame"  default="odom"/>

    <include file="$(find project2)/launch/fix.launch" />
    <node pkg="tf" type="static_transform_publisher" name="odom_tf" args="0 0 0 0 0 0 1 $(arg map_frame) $(arg odom_frame) 100" />

    <include file="$(find project2)/launch/filtering.launch" />

    <node type="rviz" name="rviz" pkg="rviz" args="-d $(find project2)/rviz/filtering.rviz" />
</launch>